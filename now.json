{
    "name": "marmt",
    "version": 2,
    "alias": "marmt.io",
    "env": {
        "MAILGUN_KEY": "@mailgunkey",
        "GMAP_KEY": "@gmapkey",
        "GANALYTICS": "@ganalytics"
    },
    "routes": [
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/api/contact",
            "dest": "api/contact/index.js"
        }
    ],
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build"
        },
        {
            "src": "api/contact/index.js",
            "use": "@now/node"
        }
    ]
}